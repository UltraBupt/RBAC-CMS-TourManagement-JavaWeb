<script type="text/javascript">
	function toSub(){
		var partner_name=$("#partner_name").val();
		var partner_desc=$("#partner_desc").val();
		var fk_partnerType_id=$("#fk_partnerType_id").val();
		var partner_position=$("#partner_position").val();
		var partner_hobby=$("#partner_hobby").val();
		if(partner_name.trim().length==0){
			alert("请输入供应商名称再进行提交");
			return false;
		}
		if(partner_desc.trim().length==0){
			alert("请输入供应商描述再进行提交");
			return false;
		}
		if(fk_partnerType_id.trim().length==0){
			alert("请选择合作商类型再进行提交");
			return false;
		}
		if(partner_position.trim().length==0){
			alert("请输入供应商职位再进行提交");
			return false;
		}
		if(partner_hobby.trim().length==0){
			alert("请输入供应商爱好再进行提交");
			return false;
		}
		$("#myForm").submit();
	}	
</script>
</head>
<body>
		<div class="portlet box yellow ">
			<div class="portlet-title">
				<div class="caption">
					<i class="fa fa-gift"></i> 新增供应商信息
				</div>
							<div class="tools">
								<a href="" class="collapse">
								</a>
								<a href="#portlet-config" data-toggle="modal" class="config">
								</a>
								<a href="" class="reload">
								</a>
								<a href="" class="remove">
								</a>
							</div>
						</div>
						<div class="portlet-body form">
						
						
							<form class="form-horizontal" action="PartnerAddServlet.shxt" method="post" id="myForm">
								<div class="form-body">
									<div class="col-md-4" style="height:150px">
										<select name="fk_partnerType_id" id="fk_partnerType_id" class="form-control input-xlarge select2me" data-placeholder="请选择供应商类型">
											<option value="">全部</option>
											<c:forEach items="${partnerType_list }" var="partnerType">
												<option value="${partnerType.partnerType_id }">${partnerType.partnerType_name }</option>
											</c:forEach>
										</select>
									</div>
									<div class="form-group">
										<div class="col-md-9">
											<input type="text" class="form-control input-sm" placeholder="合作商名称" name="partner_name" id="partner_name" value="">
										</div>
									</div>
									<div class="form-group">
										<div class="col-md-9">
											<input type="text" class="form-control input-sm" placeholder="合作商职位" name="partner_position" id="partner_position" value="">
										</div>
									</div>
									<div class="form-group">
										<div class="col-md-9">
											<input type="text" class="form-control input-sm" placeholder="合作商爱好" name="partner_hobby" id="partner_hobby" value="">
										</div>
									</div>
									<div class="form-group">
										<div class="col-md-9">
											<input type="text" class="form-control input-sm" placeholder="合作商描述" name="partner_desc" id="partner_desc" value="">
										</div>
									</div>
								</div>
								<div class="form-actions right1">
									<button type="button" class="btn yellow" value="添加供应商" onclick="toSub()">添加</button>
								</div>
							</form>
							
							
						</div>
					</div>
	
	
	
<script src="resource/assets/global/plugins/jquery-1.11.0.min.js" type="text/javascript"></script>
