<script type="text/javascript">
	function toSub(){
		$("#myForm").submit();
	}
	$(function(){
   	 var leave_status='${leave.leave_status}';
   	 $("input[name='leave_status']").each(function(){
   		 if($(this).val()==leave_status){
   			 $(this).prop("checked",true);
   			 return;
   		 }
   	 });
   });
