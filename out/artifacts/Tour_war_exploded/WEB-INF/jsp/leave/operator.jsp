							<form class="form-horizontal" action="OperatorLeaveServlet.shxt" method="post" id="myForm">
								<div class="form-body">
									<div class="radio-list">
											<label>
											<input type="radio" name="leave_status" value="1"/>同意 </label>
											<label>
											<input type="radio" name="leave_status" value="0"/>拒绝 </label>
											<label>
											<input type="radio" name="leave_status" value="-1"/>未处理 </label>
										</div>
								</div>
								<div class="form-actions right1">
									<button type="button" class="btn yellow" value="处理假条" onclick="toSub()">处理假条</button>
